{
  "name": "pr-guru",
  "version": "1.0.0",
  "private": true,
  "main": "dist",
  "type": "commonjs",
  "scripts": {
    "build": "rm -rf node_modules && rm -rf dist && yarn install && yarn transpile",
    "lint": "eslint '*/**/*.{js,ts}'",
    "lint:fix": "eslint '*/**/*.{js,ts}' --fix",
    "start": "LOCAL_DEVELOPMENT=true yarn transpile && node dist/index.js",
    "start:dev": "LOCAL_DEVELOPMENT=true tsc-watch --onSuccess \"yarn start\"",
    "transpile": "tsc"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "boom": "^7.3.0",
    "config": "^3.3.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-pino-logger": "^5.0.0",
    "lodash": "^4.17.20",
    "pino": "^6.11.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/boom": "^7.3.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/lodash": "^4.14.168",
    "@types/pino": "^6.3.5",
    "@types/request-promise": "^4.1.48",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-react": "^7.11.1",
    "pino-pretty": "^4.3.0",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  }
}
